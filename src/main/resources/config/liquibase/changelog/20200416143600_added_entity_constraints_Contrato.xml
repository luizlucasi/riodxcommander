<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Contrato.
    -->
    <changeSet id="20200416143600-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="nota_fiscal_id"
                                 baseTableName="contrato"
                                 constraintName="fk_contrato_nota_fiscal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="nota_fiscal"/>

        <addForeignKeyConstraint baseColumnNames="circuito_id"
                                 baseTableName="contrato"
                                 constraintName="fk_contrato_circuito_id"
                                 referencedColumnNames="id"
                                 referencedTableName="circuito"/>

    </changeSet>
</databaseChangeLog>
